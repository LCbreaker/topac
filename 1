class Circle:
    def __init__(self, radius):
        self._radius = radius

    @property
    def radius(self):
        return self._radius

    def area(self):
        return 3.14 * self._radius ** 2

    def circumference(self):
        return 2 * 3.14 * self._radius


class Square:
    def __init__(self, diameter):
        self._diameter = diameter

    @property
    def diameter(self):
        return self._diameter

    @property
    def side(self):
        return self._diameter / 2

    def area(self):
        return self.side ** 2


class CircleInSquare(Circle, Square):
    def __init__(self, radius):
        Circle.__init__(self, radius)
        Square.__init__(self, radius * 2)

# Пример использования
circle_in_square = CircleInSquare(5)
print(f"Circle Area: {circle_in_square.area()}")
print(f"Square Area: {circle_in_square.area()}")
print(f"Circle Circumference: {circle_in_square.circumference()}")
